{
  "name": "normaliz",
  "version": "0.2.0",
  "description": "A tiny library that normalizes data according to a schema.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "npm run bundle && npm run test",
    "bundle": "rollup src/index.js --config --file dist/index.js --format umd --name \"normaliz\" -m",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "normalize",
    "normalizer",
    "json",
    "store"
  ],
  "author": "Julien Elbaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elbywan/normaliz"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.5.2",
    "coveralls": "^3.1.0",
    "jest": "^26.5.3",
    "rollup": "^2.32.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
